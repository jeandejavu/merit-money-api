{
  "name": "merit-money-api",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Jean Melo <jeandejavu@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:dev": "rimraf dist && tsc -p tsconfig.dev.json",
    "build:watch": "yarn build:dev -w",
    "debug": "nodemon --inspect --nolazy -q -w dist dist/main/server.js",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:complete": "jest --clearCache && yarn test",
    "test:clear": "jest --clearCache",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "yarn test -- --watch -c jest-unit-config.js",
    "test:integration": "yarn test -- --watch -c jest-integration-config.js",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test -- --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info",
    "copy-files": "copyfiles -u 1 src/**/*.hbs dist/",
    "prestart": "yarn copy-files",
    "predebug": "yarn copy-files"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "handlebars": "^4.7.6",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.3",
    "multer": "^1.4.2",
    "nodemailer": "^6.9.9",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^4.1.6",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "4.17.11",
    "@types/faker": "^5.1.5",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.6.3",
    "@types/multer": "^1.4.5",
    "@types/node": "14.14.21",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "13.1.3",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.0",
    "eslint": "7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-hbs": "^1.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.1.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "mockdate": "^3.0.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "6.1.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "_moduleAliases": {
    "@": "dist/"
  },
  "engines": {
    "node": "14.x"
  }
}
